{
  "name": "ssz-data",
  "version": "0.9.0",
  "description": "",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git@github.com:statistikstadtzuerich/ssz-data.git"
  },
  "dependencies": {
    "barnard59": "git+https://github.com/zazuko/barnard59.git#master"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.2"
  },
  "scripts": {
    "clean": "rm -rf target/*",
    "get-hdb": "curl -n https://webdav.netlabs.org/ssz/HDB.csv -o input/hdb.csv",
    "get-dimensions": "curl -n https://webdav.netlabs.org/ssz/HDB_Listen.xlsx -o input/HDB_Listen.xlsx",
    "convert": "mkdir -p target && node index.js",
    "clean-dimensions": "cat target/HDB_Listen.*.nt | sed '/_:/d' | sed '/<file/d' > target/dimensions.nt",
    "clean-hdb": "cat target/hdb.nt | sed '/_:/d' > target/hdb-clean.nt",
    "put-void": "./scripts/lindas-put-void.sh",
    "post-hdb": "./scripts/lindas-post-hdb.sh",
    "post-dimensions": "./scripts/lindas-post-dimensions.sh",
    "ssz-lindas": "npm-run-all put-void post-hdb post-dimensions",
    "build": "npm-run-all clean get-hdb get-dimensions convert clean-dimensions clean-hdb put-void post-hdb post-dimensions",
    "build-local": "npm-run-all clean get-hdb get-dimensions convert clean-dimensions clean-hdb"
  }
}
